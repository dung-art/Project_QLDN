package com.WebOfNVD.User.Repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.JpaSpecificationExecutor;
import org.springframework.data.jpa.repository.Query;

import com.WebOfNVD.User.Entity.PartnerUser;

public interface PartnerUserRepo extends JpaRepository<PartnerUser, String> , JpaSpecificationExecutor<PartnerUser>{
	@Query(value = "Select s from PartnerUser s Where s.isDelete = false")
	List<PartnerUser> findAllNoDelete();
	@Query(value = "Select s from PartnerUser s Where s.isDelete = true")
	List<PartnerUser> findAllDeleted();
}
